{
    "name": "perafan/cashier-openpay",
    "description": "Laravel Cashier provides an expressive, fluent interface to Openpay's subscription billing services.",
    "keywords": ["laravel", "billing", "cashier", "openpay"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pedro Peraf√°n Carrasco",
            "email": "pedro.perafan.carrasco@gmail.com"
        }
    ],
    "homepage": "https://github.com/Perafan18/cashier-openpay",
    "require": {
        "illuminate/support": "~5|~6|~7",
        "openpay/sdk": "dev-master",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Perafan\\CashierOpenpay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Perafan\\CashierOpenpay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Perafan\\CashierOpenpay\\CashierOpenpayServiceProvider"
            ],
            "aliases": {
                "CashierOpenpay": "Perafan\\CashierOpenpay\\Facades\\CashierOpenpay"
            }
        }
    }
}
